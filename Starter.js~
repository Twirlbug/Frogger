function restart(){
	window.location="ChooseSprite.html";
}

function beginGame(){
var enemynumber= document.getElementById("EnemyNum").value;
var e = document.getElementById("selecter");
var sprite = e.options[e.selectedIndex].value;
settings.usedsprite=sprite;
if (enemynumber>1){
settings.enemies=enemynumber;}
else {settings.enemies=1;}
window.location="index.html";
}

var settings={
enemies:1,
usedsprite:1
};
